    <description>Create an Academic Alert and assign to Case Owner if student Term Grade, Numerical Grade, drops below 75</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>&quot;Grade has dropped to &quot; &amp; TEXT([hed__Term_Grade__c].hed__Numerical_Grade__c) &amp; &quot;.&quot;</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_3568202223</name>
        <dataType>String</dataType>
        <expression>&quot;Grade has dropped to &quot; &amp; TEXT({!myVariable_current.hed__Numerical_Grade__c}) &amp; &quot;.&quot;</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
    [hed__Term_Grade__c].hed__Numerical_Grade__c &lt; 75,
    NOT(ISBLANK([hed__Term_Grade__c].hed__Numerical_Grade__c)),
    OR(
        ISBLANK(PRIORVALUE([hed__Term_Grade__c].hed__Numerical_Grade__c)),
        PRIORVALUE([hed__Term_Grade__c].hed__Numerical_Grade__c) &gt;= 75,
        AND(
            [hed__Term_Grade__c].hed__Numerical_Grade__c &lt;&gt; 0,
            OR(
                PRIORVALUE([hed__Term_Grade__c].hed__Numerical_Grade__c) == 0,
                ISNEW()
            )
        )
    )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
    {!myVariable_current.hed__Numerical_Grade__c} &lt; 75,
    NOT(ISBLANK({!myVariable_current.hed__Numerical_Grade__c})),
    OR(
        ISBLANK(PRIORVALUE({!myVariable_current.hed__Numerical_Grade__c})),
        PRIORVALUE({!myVariable_current.hed__Numerical_Grade__c}) &gt;= 75,
        AND(
            {!myVariable_current.hed__Numerical_Grade__c} &lt;&gt; 0,
            OR(
                PRIORVALUE({!myVariable_current.hed__Numerical_Grade__c}) == 0,
                ISNEW()
            )
        )
    )
)</expression>
